<%- presenter.identifiers.each_with_index do |id, idx| -%>
<tr>
  <% if idx.zero? %>
  <th rowspan="<%= presenter.identifiers.size %>">
    <%= t('blacklight.search.fields.identifier') %>
  </th>
  <% end %>
  <td class="attribute attribute-identifier<%= id.prefix ? "attribute-identifier_#{prefix}" : '' %>">
    <% if id.prefix %>
      <span class="label label-default"><%= t("spot.identifiers.labels.#{id.prefix}") %></span>
    <% end %>
    <%= id.value %>
  </td>
</tr>
<%- end -%>
